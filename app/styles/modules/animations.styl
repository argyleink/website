@keyframes eye-blink
  0%
    transform: scaleY(-1)
  96%
    transform: scaleY(-1)
  97%
    transform: scaleY(-0.1)
  99%
    transform: scaleY(-0.1)
  100%
    transform: scaleY(-1)

@keyframes bounce-down
  0%
    transform: translateY(0)
  50%
    transform: translateY(8px)
  100%
    transform: translateY(0)
    
@keyframes look-left
  0%
    left: 55px
  80%
    left: 55px
  81%
    left: 53px
  99%
    left: 52px
  100%
    left: 55px