.page
  display none
  
  &.show
    display block
    overflow scroll

.pageload-overlay
  position fixed
  width 100%
  height 100%
  top 0
  left 0
  visibility hidden
  z-index 10
  
  &.show
    visibility visible
  svg
    position absolute
    top 0
    left 0
    pointer-events none
    path
      fill hsl(0,0%,10%)
